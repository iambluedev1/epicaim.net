{
  "name": "EpicAim",
  "slug": "epicaim",
  "author": [{
    "name": "Orphevs"
  }],
  "version": "1.0.0",
  "requirements" : {
    "cms": ">= 1.0.0"
  },
  "flash": {
    "class": {
      "error": "danger",
      "success": "success",
      "info": "info",
      "warning": "warning"
    },
    "dismissable": {
      "template": "<div class=\"alert alert-%s alert-dismissable\"><button type=\"button\" class=\"close\" data-dismiss=\"alert\" aria-hidden=\"true\">&times;</button> %s</div>"
    },
    "nodismissable": {
      "template": "<div class=\"alert alert-%s\"> %s</div>"
    }
  },
  "translate": {
    "parts" : {
      "header": "header",
      "footer": "footer"
    },
    "supported": {
      "fr": {
        "contributors": [{
           "name": "iambludev"
        }]
      },
      "en": {
        "contributors": [{
           "name": "iambludev"
        }]
      }
    }
  }
}